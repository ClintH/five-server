{
  "name": "five-server",
  "version": "0.0.1",
  "description": "Development Server with Live Reload Capability. (Maintained Fork of Live-Server)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "live-server": "./lib/live-server.js",
    "five-server": "./lib/live-server.js"
  },
  "keywords": [
    "front-end",
    "development",
    "tool",
    "live",
    "http",
    "server",
    "cli",
    "reload",
    "refresh"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "tsc --watch",
    "build": "npm run extract && tsc",
    "format": "prettier --check src/**",
    "format:write": "prettier --write src/**",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx",
    "extract": "extract version name src/const.ts",
    "test": "jest --forceExit --silent",
    "live-server": "node ./lib/live-server.js --no-browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yandeu/five-server.git"
  },
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yandeu/five-server/issues"
  },
  "homepage": "https://github.com/yandeu/five-server#readme",
  "dependencies": {
    "chokidar": "^3.5.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "2.6.9",
    "event-stream": "^4.0.1",
    "finalhandler": "1.1.2",
    "http-auth": "^4.1.3",
    "http-auth-connect": "^1.0.4",
    "morgan": "^1.9.1",
    "object-assign": "^4.1.1",
    "open": "^8.0.2",
    "parseurl": "~1.3.3",
    "proxy-middleware": "^0.15.0",
    "send": "^0.17.1",
    "serve-index": "^1.9.1",
    "utils-merge": "1.0.1",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@actions/exec": "^1.0.4",
    "@types/node": "^14.14.35",
    "@types/ws": "^7.4.0",
    "@yandeu/eslint-config": "0.0.1",
    "@yandeu/extract": "0.0.3",
    "@yandeu/prettier-config": "0.0.1",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12"
  },
  "funding": {
    "url": "https://github.com/sponsors/yandeu"
  }
}
